<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity_old" >

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/imageButton3"
        android:layout_marginRight="43dp"
        android:onClick="onNotTakenButtonClick"
        android:src="@drawable/not_taken" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView5"
        android:layout_alignBottom="@+id/textView5"
        android:layout_alignLeft="@+id/imageButton3"
        android:layout_marginLeft="16dp"
        android:text="@string/took"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/imageButton4"
        android:layout_below="@+id/imageButton4"
        android:text="@string/nottaken"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/TextView01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/TextView02"
        android:layout_alignBottom="@+id/TextView02"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="26dp"
        android:text="Refill Checkin"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/TextView02"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/ImageButton01"
        android:layout_marginRight="20dp"
        android:text="Office Checkin"
        android:textSize="20sp" />

    <ImageButton
        android:id="@+id/ImageButton01"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignLeft="@+id/textView5"
        android:layout_below="@+id/textView5"
        android:layout_marginTop="28dp"
        android:onClick="onRefillButtonClick"
        android:src="@drawable/appointment_take" />

    <ImageButton
        android:id="@+id/ImageButton02"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/TextView01"
        android:layout_alignLeft="@+id/imageButton3"
        android:onClick="onOfficeButtonClick"
        android:src="@drawable/refill_take" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/imageButton2"
        android:text="   Snooze"
        android:textSize="20sp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/textView3"
        android:scaleType="fitCenter"
        android:layout_alignLeft="@+id/imageButton4"
        android:onClick="onAlarmButtonClick"
        android:src="@drawable/snooze" />

    <ImageButton
        android:id="@+id/imageButton1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_above="@+id/textView2"
        android:layout_alignLeft="@+id/TextView01"
        android:layout_marginLeft="14dp"
        android:onClick="onPillCamButtonClick"
        android:src="@drawable/pill_cam" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/imageButton3"
        android:layout_alignLeft="@+id/imageButton1"
        android:layout_marginBottom="28dp"
        android:text="   Pillcam"
        android:textSize="20sp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:scaleType="fitCenter"
        android:layout_alignLeft="@+id/textView2"
        android:layout_centerVertical="true"
        android:onClick="onTookButtonClick"
        android:src="@drawable/took" />

</RelativeLayout>